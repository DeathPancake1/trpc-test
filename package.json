{
  "name": "@examples/minimal",
  "private": true,
  "version": "10.34.0",
  "scripts": {
    "build": "tsc",
    "dev:server": "tsx watch server",
    "dev:client": "wait-port 3000 && cd client && npm start",
    "dev": "run-p dev:* --print-label",
    "start": "node dist/server",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives */*.ts",
    "test-dev": "start-server-and-test 'tsx server' 3000 'npm run dev:client'",
    "test-start": "start-server-and-test 'node dist/server' 3000 'npm run dev:client'"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "@types/node": "^18.16.16",
    "eslint": "^8.40.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "wait-port": "^1.0.1"
  },
  "dependencies": {
    "next": "^13.4.9",
    "react": "^18.2.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.14.1",
    "trpc-panel": "^1.3.4"
  }
}
